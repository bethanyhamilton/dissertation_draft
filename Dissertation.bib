	pages = {137--159},
	file = {EBSCO Full Text:C\:\\Users\\betha\\Zotero\\storage\\ATZ3T8PT\\Higgins et al. - 2009 - A re-evaluation of random-effects meta-analysis.pdf:application/pdf},
}

@article{viechtbauer2007,
